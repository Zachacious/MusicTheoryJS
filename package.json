{
  "name": "musictheoryjs",
  "version": "2.0.1",
  "description": "A music theory library for JavaScript",
  "source": "src/index.ts",
  "main": "dist/musictheory.js",
  "module": "dist/module.js",
  "types": "dist/musictheory.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint -c .eslintrc.js *.ts",
    "clean": "rimraf dist",
    "build": "run-s clean production",
    "production": "rollup -c",
    "gen-tables": "set GENTABLES=true && node ./GenerateTables.js",
    "build-docs": "npx typedoc"
  },
  "keywords": [
    "music",
    "theory",
    "note",
    "scale",
    "interval",
    "chord",
    "key",
    "progression",
    "mode",
    "major",
    "minor"
  ],
  "babel": {
    "env": {
      "test": {
        "plugins": [
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ]
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zachacious/MusicTheoryJS.git"
  },
  "author": "Zach Moore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zachacious/MusicTheoryJS/issues"
  },
  "homepage": "https://github.com/Zachacious/MusicTheoryJS#readme",
  "dependencies": {
    "@types/node": "^17.0.5",
    "global": "^4.4.0",
    "ts-debounce": "^4.0.0",
    "typedoc": "^0.22.10",
    "uid": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "http-server": "^14.0.0",
    "jest": "^27.3.1",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-typescript2": "^0.31.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.5.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "files": [
    "/dist"
  ]
}
